/**index.wxss**/
page{
  background: #fff;
}
.topPanel{
  position: relative;
  width: 750rpx;
  height: 200rpx;
  background: #518FF5;
}
.directionTitle{
  display: block;
  font-size: 100rpx;
  width: 710rpx;
  line-height: 200rpx;
  text-align: right;
  color: white;
  letter-spacing: 50rpx;
}
.topImg{
  position: absolute;
  width: 300rpx;
  height: 300rpx;
  top: 50rpx;
  left: 50rpx;
  border-radius: 20rpx;
}
.introduceContainer{
  position: relative;
  width: 750rpx;
  height: 300rpx;
  padding-top: 100rpx;
  overflow-y:hidden;
  text-indent: 400rpx;
}
.introduceContainerOn{
  position: relative;
  width: 750rpx;
  height: auto;
  padding-top: 100rpx;
  overflow-y:hidden;
  text-indent: 400rpx;
}

.introduce{
  font-size: 40rpx;
  color: #518FF5;
  line-height: 60rpx;
}
.downButton{
  position: relative;
  height: 120rpx;
  width: 750rpx;
  padding: 0;
}
.iconContainer{
  position: absolute;
  width: 120rpx;
  height: 120rpx;
  left: 325rpx;
}
.downIcon{
  width: 120rpx;
  height: 120rpx;
}
.memberItem image{
  border-radius: 25rpx;
  margin-right: 20rpx;
}
.stackPage{
  position: relative;
  width: 750rpx;
  height: auto;
  background: #EFEFEF;
}
.stackItem{
  position: relative;
  display: inline-block;
  width: 200rpx;
  height: 300rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
  margin-top: 25rpx;
  margin-bottom: 25rpx;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0 0 10rpx gray;
}
.stackItemImage{
  position: absolute;
  width: 150rpx;
  height: 150rpx;
  left: 25rpx;
  top: 30rpx;
}
.stackItemTitle{
  position: absolute;
  width: 200rpx;
  top: 220rpx;
  text-align: center;
}